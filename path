BIN_COMPOSER="~/.composer/vendor/bin"
BIN_HOME="~/bin"
BIN_PG_APP="/Applications/Postgres.app/Contents/Versions/9.4/bin"
BIN_PHP="$(brew --prefix php55)/bin"
BIN_LOCAL="/usr/local/bin"
BIN_LOCAL_SYS="/usr/local/sbin"

export PATH="$BIN_COMPOSER:$BIN_HOME:$BIN_PG_APP:$BIN_PHP:$BIN_LOCAL:$BIN_LOCAL_SYS:$BIN_RUBY:$BIN_GNU:$PATH"
