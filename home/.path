BIN_COMPOSER="~/.composer/vendor/bin"
BIN_HOME="~/bin"
BIN_PHP="$(brew --prefix php55)/bin"
BIN_PG_APP="/Applications/Postgres93.app/Contents/MacOS/bin"
BIN_LOCAL="/usr/local/bin"
BIN_LOCAL_SYS="/usr/local/sbin"
BIN_RUBY="/usr/local/opt/ruby/bin"

export PATH="$BIN_COMPOSER:$BIN_HOME:$BIN_PHP:$BIN_PG_APP:$BIN_LOCAL:$BIN_LOCAL_SYS:$BIN_RUBY:$PATH"
